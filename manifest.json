{
  "name": "Unoffical Anilist Extension",
  "description": "Easily update your list from an extension.",
  "version": "1.1",
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup/index.html"
  },
  "permissions": ["storage"]
}
