{
	"name": "Unoffical Anilist Extension",
	"description": "Easily update your list from an extension.",
	"version": "2.0.1",
	"manifest_version": 2,
	"icons": {
		"48": "icon.svg"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_icon": "icon.svg",
		"default_popup": "index.html"
	},
	"permissions": ["storage"]
}
